<section class="brand-carousel-wrap" data-section-id="{{ section.id }}">
  <div class="brand-carousel">
    <div class="container">
      <div class="swiper logoSwiper">
        <ul class="client-testimonials-swiper swiper-wrapper">
          {% for block in section.blocks %}
            <li class="client-testimonial-body swiper-slide">
              {{ block.settings.brand_logo }}
            </li>
          {% endfor %}
        </ul>
      </div>
    </div>
  </div>
</section>
{% schema %}
{
  "name": "carousel",
  "presets": [
    {
      "name": "Carousel"
    }
  ],
  "blocks": [
    {
      "name": "brand_logo",
      "type": "brand_logo",
      "limit": 10,
      "settings": [
        {
          "type": "textarea",
          "id": "brand_logo",
          "label": "Brand Logo"
        }
      ]
    }
  ]
}
{% endschema %}
